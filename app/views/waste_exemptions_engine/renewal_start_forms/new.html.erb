<div class="govuk-grid-row">
  <div class="govuk-grid-column-two-thirds">
    <%= form_for(@renewal_start_form) do |f| %>
      <%= render partial: "waste_exemptions_engine/shared/error_summary", locals: { f: f } %>

      <h1 class="govuk-heading-l"><%= t(".heading") %></h1>

      <%= render("waste_exemptions_engine/shared/data_overview/company_info", form: @renewal_start_form) %>
      <%= render("waste_exemptions_engine/shared/data_overview/registration_info", form: @renewal_start_form) %>

      <%= f.govuk_submit t(".next_button") %>
    <% end %>
  </div>
</div>
